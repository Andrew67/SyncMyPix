<LinearLayout android:id="@+id/LinearLayout01" 
	android:layout_width="fill_parent" 
	android:layout_height="fill_parent" 
	android:orientation="vertical"
	xmlns:android="http://schemas.android.com/apk/res/android">
	
	<TextView android:text="Started" 
		android:id="@+id/label1" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" android:textSize="18dip" android:textColor="#FFFFFF"/>
		
	<TextView android:text="" 
		android:id="@+id/started" 
		android:layout_height="wrap_content" 
		android:layout_width="fill_parent" />

	<TextView android:text="Completed" 
		android:id="@+id/label2" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" android:textSize="18dip" android:textColor="#FFFFFF"/>
		
	<TextView android:text="" 
		android:id="@+id/completed" 
		android:layout_height="wrap_content" 
		android:layout_width="fill_parent" />

	<ImageView android:id="@+id/contactImage"
		android:layout_height="125dip"
		android:layout_width="fill_parent" android:padding="10dip"/>
		 	
	<TextView android:text="@string/selectedName" 
		android:id="@+id/selectedName" 
		android:layout_height="wrap_content" 
		android:layout_width="fill_parent" 
		android:gravity="center_horizontal" android:paddingBottom="4dip"/>
		
	<TextView android:text="@string/resultsList" 
		android:id="@+id/resultsListHeader" 
		android:layout_height="wrap_content" 
		android:layout_width="fill_parent" 
		android:textSize="18dip" android:textColor="#FFFFFF" />
		
	<ListView android:id="@+id/resultList" 
		android:layout_width="fill_parent" 
		android:layout_height="wrap_content" />
	
	
</LinearLayout>
